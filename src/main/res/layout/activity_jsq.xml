<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".JSQ">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="70px">

        <TableRow>

            <EditText
                android:id="@+id/input"

                android:layout_width="match_parent"
                android:layout_height="170px"
                android:layout_marginBottom="20px"
                android:layout_span="7"
                android:background="#f79f99e7"
                android:editable="false"
                android:gravity="right|bottom" />
        </TableRow>

        <TableRow>

            <EditText
                android:id="@+id/output"
                android:layout_width="match_parent"
                android:layout_height="170px"
                android:layout_marginBottom="20px"
                android:layout_span="6"
                android:background="#ffffff"
                android:editable="false"
                android:gravity="right|bottom" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/kaifang"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="âˆš" />

            <Button
                android:id="@+id/jiecheng"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="x!" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/C"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="C" />

            <Button
                android:id="@+id/CE"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="CE" />

            <Button
                android:id="@+id/delete"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="delete" />

            <Button
                android:id="@+id/sin"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="sin" />

            <Button
                android:id="@+id/jia"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="+" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/num7"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="7" />

            <Button
                android:id="@+id/num8"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="8" />

            <Button
                android:id="@+id/num9"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="9" />

            <Button
                android:id="@+id/cos"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="cos" />

            <Button
                android:id="@+id/jian"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="-" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/num4"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="4" />

            <Button
                android:id="@+id/num5"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="5" />

            <Button
                android:id="@+id/num6"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="6" />

            <Button
                android:id="@+id/tan"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="tan" />

            <Button
                android:id="@+id/cheng"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="*" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/num1"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="1" />

            <Button
                android:id="@+id/num2"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="2" />

            <Button
                android:id="@+id/num3"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="3" />

            <Button
                android:id="@+id/num0"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_num"
                android:text="0" />

            <Button
                android:id="@+id/chu"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:background="@drawable/button_1"
                android:text="/" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/dian"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:layout_span="2"
                android:background="@drawable/button_1"
                android:text="." />

            <Button
                android:id="@+id/deng"
                android:layout_width="150px"
                android:layout_height="wrap_content"
                android:layout_margin="10px"
                android:layout_span="2"
                android:background="@drawable/deng_1"
                android:text="=" />
        </TableRow>

    </TableLayout>


</android.support.constraint.ConstraintLayout>